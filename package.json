{
  "name": "angular2_app",
  "version": "1.0.0",
  "description": "A webpack starter for angular2 app",
  "scripts": {
    "build:aot": "npm run clean:aot && npm run ngc && cross-env NODE_ENV=aot webpack --progress && npm run clean:unbundle",
    "build:prod": "npm run clean:prod && cross-env NODE_ENV=prod webpack --progress --profile --bail && npm run clean:unbundle",
    "clean:prod": "rimraf dist",
    "clean:aot": "rimraf dist rimraf aot",
    "clean:unbundle":"rimraf dist/unbundled",
    "mock": "json-server --watch mock/db.json --port 8888",
    "ngc": "ngc -p tsconfig.aot.json --locale=en-US",
    "prestart": "npm run scss",
    "scss": "node-sass --output-style compressed -o src/ src/",
    "scss:watch": "node-sass --output-style compressed -o src/ src/ --watch",
    "server:aot": "npm run clean:aot && npm run ngc && cross-env NODE_ENV=aot npm run devServer",
    "server:dev": "cross-env NODE_ENV=dev npm run devServer",
    "server:prod": "npm run clean:prod && npm run ngc && cross-env NODE_ENV=prod npm run devServer",
    "start": "concurrently \"npm run scss:watch\" \"npm run server:dev\"",
    "test": "karma start",
    "devServer": "webpack-dev-server --progress"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": ">=2.0.0",
    "@angular/compiler": ">=2.0.0",
    "@angular/core": ">=2.0.0",
    "@angular/forms": ">=2.0.0",
    "@angular/http": ">=2.0.0",
    "@angular/platform-browser": ">=2.0.0",
    "@angular/platform-browser-dynamic": ">=2.0.0",
    "@angular/platform-server": ">=2.0.0",
    "@angular/router": ">=3.0.0",
    "core-js": "^2.4.1",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^0.6.2",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.2.34",
    "@types/node": "^6.0.40",
    "angular2-load-children-loader": "^0.1.3",
    "angular2-router-loader": "^0.3.3",
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.4.1",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.23.1",
    "css-to-string-loader": "^0.1.1",
    "es6-promise-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.15.0",
    "image-webpack-loader": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.21",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.10.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.11.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^0.8.1",
    "typescript": "^2.0.2",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.14.0"
  }
}
